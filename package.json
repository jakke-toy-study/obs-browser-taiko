{
  "name": "obs-browser-kit",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm run build-content && tsc && concurrently \"vite\" \"wait-on tcp:3000 && electron-forge start\"",
    "build-content": "vite build --config vite.dev.webcontents.ts",
    "package": "tsc && vite build && electron-forge package",
    "make": "tsc && vite build && electron-forge make",
    "publish": "tsc && vite build && electron-forge publish",
    "svgr": "npx @svgr/cli -d ./assets/svg --ignore-existing --typescript --no-dimensions ./assets/svg/tsx"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@types/electron": "^1.6.12",
    "@types/express": "^5.0.1",
    "@types/glob": "^8.0.0",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "electron": "^33.0.2",
    "postcss": "^8.4.47",
    "prettier": "^3.5.3",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-svgr": "^4.3.0",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "express": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ws": "^8.18.1"
  },
  "overrides": {
    "minimatch": "5.1.2",
    "glob": "8.1.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start",
            "author": "jakkelab",
            "description": "none"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ]
    }
  }
}
